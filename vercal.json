{
    "version": 2,
    "builds": [
        {
          "src": "app/main.py",
          "use": "@vercel/python",
          "config": {
            "maxLambdaSize": "50mb"
          }
        }
      ],
    "functions": {
      "app/main.py": {
        "maxDuration": 120
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.main:app"
      }
    ]
}

